{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
     "noImplicitAny": false,
   
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
